@model MVCDemo.Models.Employee

@{
    ViewBag.Title = "Employee Details";
}

<h2>Employee Details</h2>
<table style="font-familty:Arial" border ="1">
    <tr>
        <th>Employee Id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>City</th>
